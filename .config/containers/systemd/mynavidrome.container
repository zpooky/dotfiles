[Install]
WantedBy = default.target

# mynavidrome.container
[Container]
ContainerName=mynavidrome
Image=docker.io/deluan/navidrome:latest
PublishPort=4533:4533
Volume=/tank-spooky/navidrome-data:/data:z
Volume=/tank-spooky/music:/music:ro,z
AutoUpdate=registry
Environment=PUID=1000 PGID=1001 TZ=Etc/UTC
